//fontastic
@font-face
  font-display: swap;
  font-family "odecopack"
  src url("../fonts/odecopack.eot")
  src url("../fonts/odecopack.eot?#iefix") format("embedded-opentype"), url("../fonts/odecopack.woff") format("woff"), url("../fonts/odecopack.ttf") format("truetype"), url("../fonts/odecopack.svg#odecopack") format("svg")

font-weight normal
font-style normal
[data-icon]:before
  font-family "odecopack" !important
  content attr(data-icon)
  font-style normal !important
  font-weight normal !important
  font-variant normal !important
  text-transform none !important
  speak none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

[class^="icon-"]:before,
[class*=" icon-"]:before
  font-family "odecopack" !important
  font-style normal !important
  font-weight normal !important
  font-variant normal !important
  text-transform none !important
  speak none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-belt
  &:before
    content "\61"

.icon-robot
  &:before
    content "\62"

.icon-belt-2
  &:before
    content "\63"

.icon-robot-1
  &:before
    content "\64"

.icon-robot-2
  &:before
    content "\65"

.icon-dashboard-1
  &:before
    content "\66"

.icon-dashboard
  &:before
    content "\67"